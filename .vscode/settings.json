{
  "python.languageServer": "Pylance",
  "emmet.includeLanguages": {
    "django-html": "html"
  },
  "editor.defaultFormatter": "batisteo.vscode-django",
  "sqltools.connections": [
    {
      "previewLimit": 50,
      "driver": "SQLite",
      "database": "${workspaceFolder:django_project}/db.sqlite3",
      "name": "SQLitte"
    }
  ],
  "sqltools.useNodeRuntime": true
}
